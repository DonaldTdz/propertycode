@using YK.FrameworkTools.PluginService;
<div id="divMonthReportIndexController" ng-controller="MonthReportIndexController">
    <div class="col-md-12">
        <ul class="nav nav-tabs" role="tablist" id="moduleTab">
            <li class="active">
                <a href="#tabMonthReport" role="tab" data-toggle="tab" onclick="MonthReportClick()" class="ng-binding">汇总表</a>
            </li>
            <li><a href="#tabMonthReportDetail" role="tab" data-toggle="tab" onclick="MonthReportDetailClick()" class="ng-binding">明细表</a></li>
        </ul>
        <div class="tab-content">
            <div class="tab-pane active" id="tabMonthReport">
                <div id="divMonthReportContainer"></div>
            </div>
            <div class="tab-pane" id="tabMonthReportDetail">
                <div id="divMonthReportDetailContainer"></div>
            </div>
        </div>
    </div>
    <script type="text/javascript">
        var deptContainerService = ckFramework.GetAngularService('DeptContainerService');
        $(document).ready(function () {
            MonthReportClick();
        });
        //汇总表
        function MonthReportClick() {
            deptContainerService.ShowContainerContent('divMonthReportContainer', 'PropertyMgr/Report/MonthReportContainerIndex', ['apps/HomeApp', '/Plugins/PropertyMgr/Scripts/controllers/Report/MonthReportContainerController.js'], 'divMonthReportContainerController');
        }
        //明细表
        function MonthReportDetailClick() {
            deptContainerService.ShowContainerContent('divMonthReportDetailContainer', 'PropertyMgr/Report/MonthReportDetailIndex', ['apps/HomeApp', '/Plugins/PropertyMgr/Scripts/controllers/Report/MonthReportDetailController.js'], 'divMonthReportDetailController');
        }

    </script>
</div>


