<div id="divArrearsReportIndexController" ng-controller="ArrearsReportIndexController">
    <div class="col-md-12">
        <ul class="nav nav-tabs" role="tablist" id="moduleTab">

            <li class="active">
            <a href="#tabArrearsSummaryReport" role="tab" data-toggle="tab" onclick="ArrearsSummaryReport()" class="ng-binding">汇总表</a></li>
            <li>  <a href="#tabArrearsDetailReport" role="tab" data-toggle="tab" onclick="ArrearsDetailReport()" class="ng-binding">明细表</a></li>
        </ul>


        <div class="tab-content">
            <div class="tab-pane active" id="tabArrearsSummaryReport">
                <div id="divArrearsReportContainer"></div>
            </div>
   
            <div class="tab-pane active" id="tabArrearsDetailReport">
                <div id="divArrearsDetailReportContainerIndex"></div>
            </div>

        </div>
    </div>
    <script type="text/javascript">
        $(document).ready(function () {
            ArrearsSummaryReport();

        });

       // var deptContainerService = ckFramework.GetAngularService('ArrearsReportIndexService');

        //汇总表
        function ArrearsSummaryReport() {

            var deptContainerService = ckFramework.GetAngularService('ReportService');
            deptContainerService.ShowContainerContent('divArrearsReportContainer', 'PropertyMgr/Report/ArrearsReportContainerIndex', ['apps/HomeApp', '/Plugins/PropertyMgr/Scripts/controllers/Report/ArrearsReportContainerController.js'], 'divArrearsReportContainerController');
        }
        //明细表
        function ArrearsDetailReport()
        {

            var deptContainerService = ckFramework.GetAngularService('ReportService');
            deptContainerService.ShowContainerContent('divArrearsDetailReportContainerIndex', 'PropertyMgr/Report/ArrearsDetailReportContainerIndex', ['apps/HomeApp', '/Plugins/PropertyMgr/Scripts/controllers/Report/ArrearsDetailReportContainerController.js'], 'divArrearsDetailReportContainerController');
        }


    </script>
</div>
