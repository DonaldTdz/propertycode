<div id="divDataInitIndexController" >
    <div class="col-md-12">
        <ul class="nav nav-tabs" role="tablist" id="moduleTab">
            <li class="active">
                <a href="#tabPrepayAccount" role="tab" data-toggle="tab" onclick="PrepayAccountClick()" class="ng-binding">导入预交费信息</a>
            </li>
            <li><a href="#tabDataInit" role="tab" data-toggle="tab" onclick="DataInitClick()" class="ng-binding">导入欠费信息</a></li>
            <li><a href="#tabHistoryCost" role="tab" data-toggle="tab" onclick="HistoryCostClick()" class="ng-binding">导入历史缴费信息</a></li>
        </ul>

        <div class="tab-content">
            <div class="tab-pane active" id="tabPrepayAccount">
                <div id="divPrepayAccountContainer"></div>
            </div>
            <div class="tab-pane" id="tabDataInit">
                <div id="divDataInitContainer"></div>
            </div>
            <div class="tab-pane" id="tabHistoryCost">
                <div id="divHistoryCostContainer"></div>
            </div>
        </div>
    </div>
    <script type="text/javascript">
        $(document).ready(function () {
            PrepayAccountClick();
        });
        //导入预交费信息
        function PrepayAccountClick() {
            deptContainerService.ShowContainerContent('divPrepayAccountContainer', 'PropertyMgr/PrepayAccount/index', ['apps/HomeApp', '/Plugins/PropertyMgr/Scripts/controllers/PrepayAccount/PrepayAccountListController.js'], 'divPrepayAccountListController');
        }
        //导入欠费信息
        function DataInitClick() {
            deptContainerService.ShowContainerContent('divDataInitContainer', 'PropertyMgr/DataInit/ImportArrearageIndex', ['apps/HomeApp', '/Plugins/PropertyMgr/Scripts/controllers/DataInit/ImportArrearageIndexController.js'], 'divImportArrearageIndexController');
        }
        //导入历史费用信息
        function HistoryCostClick() {
            deptContainerService.ShowContainerContent('divHistoryCostContainer', 'PropertyMgr/DataInit/ImportHistoryCostIndex', ['apps/HomeApp', '/Plugins/PropertyMgr/Scripts/controllers/DataInit/ImportHistoryCostIndexController.js'], 'divImportHistoryCostIndexController');
        }
    </script>
</div>
