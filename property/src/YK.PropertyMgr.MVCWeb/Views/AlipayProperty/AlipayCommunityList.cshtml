@using YK.FrameworkTools.PluginService;
<div id="divAlipayCommunityListController" ng-controller="AlipayCommunityListController">


    <div class="row">
        <div class="col-md-12">
            <table class="table table-striped">
                <tbody>
                    <tr>
                        <th>操作</th>
                        <th>小区</th>
                        <th>创建时间</th>
                        <th>状态</th>

                    </tr>
                    <tr ng-repeat="item in AlipayCommunityList">
                        <td ng-show="item.AlipayCommunityId.length>0"><a style="cursor: pointer;" ng-click="AlipayCreateCommunity('Edit',item.Id)"> 修改</a>/<a style="cursor: pointer;" ng-click="AlipayCreateCommunity('Show',item.Id)"> 查看</a></td>
                        <td ng-show="item.AlipayCommunityId.length<=0"><a style="cursor: pointer;" ng-click="AlipayCreateCommunity('Add',item.ComDeptId)"> 接入</a></td>
                        <td>{{item.CommunityName }}</td>
                        <td>{{item.CreateTimeStr }}</td>
                        <td>{{item.StatusStr}}</td>

                    </tr>
                    <tr ng-show="AlipayCommunityList.length == 0">
                        <td colspan="11" style="text-align:center;">没有数据</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>


</div>

